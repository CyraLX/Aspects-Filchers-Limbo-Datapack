{
    "type": "minecraft:multi_action",
    "title": {
        "translate": "dialog.aspects.configs.entry",
        "fallback": "!! Missing Resource Pack for Aspects !!",
        "with": [
            {
                "translate": "filched",
                "fallback": "!! Missing Resource Pack for Aspects: Filcher's Limbo !!",
                "color": "blue"
            },
            {
                "translate": "config.filched.filcher.limbo_generation_range",
                "fallback": "!! Missing Resource Pack for Aspects: Filcher's Limbo !!"
            }
        ]
    },
    "external_title": {
        "translate": "dialog.aspects.configs.entry.external_title",
        "fallback": "!! Missing Resource Pack for Aspects !!",
        "with": [
            {
                "translate": "filched",
                "fallback": "!! Missing Resource Pack for Aspects: Filcher's Limbo !!",
                "color": "blue"
            },
            {
                "translate": "config.filched.filcher.limbo_generation_range",
                "fallback": "!! Missing Resource Pack for Aspects: Filcher's Limbo !!"
            }
        ]
    },
    "body": [
        {
            "type": "minecraft:plain_message",
            "width": 450,
            "contents": {
                "translate": "config.filched.filcher.limbo_generation_range.description",
                "extra": [
                    "\n",
                    {
                        "translate": "config.filched.filcher.limbo_generation_range.note",
                        "color": "#d6d6d6"
                    }
                ]
            }
        },
        {
            "type": "minecraft:plain_message",
            "width": 450,
            "contents": {
                "text": "",
                "extra": [
                    {
                        "translate": "dialog.aspects.configs.entry.accepts_values",
                        "with": [
                            {
                                "text": "",
                                "color": "#d6d6d6",
                                "extra": [
                                    {
                                        "translate": "dialog.aspects.configs.entry.accepts_values.above_equal",
                                        "color": "blue",
                                        "with": [
                                            "0"
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    "\n",
                    {
                        "translate": "dialog.aspects.configs.entry.default",
                        "with": [
                            {
                                "text": "10000",
                                "color": "blue"
                            }
                        ]
                    }
                ]
            }
        }
    ],
    "inputs": [
        {
            "type": "minecraft:number_range",
            "label": {
                "translate": "dialog.aspects.configs.entry.change_value"
            },
            "key": "value",
            "width": 225,
            "start": 100,
            "end": 250000,
            "initial": 10000,
            "step": 100
        }
    ],
    "can_close_with_escape": true,
    "pause": true,
    "exit_action": {
        "label": {
            "translate": "gui.back"
        },
        "action": {
            "type": "minecraft:show_dialog",
            "dialog": "aspects:menu/configs"
        }
    },
    "columns": 2,
    "actions": [
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.apply_input_value"
            },
            "action": {
                "type": "minecraft:dynamic/run_command",
                "template": "scoreboard players set #filched filched.config.filcher.limbo_generation_range $(value)"
            }
        },
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.reset_value"
            },
            "action": {
                "type": "minecraft:run_command",
                "command": "function filched:config/filcher/limbo_generation_range/default"
            }
        },
        {
            "label": {
                "translate": "dialog.aspects.configs.entry.check_value"
            },
            "action": {
                "type": "minecraft:run_command",
                "command": "function filched:config/filcher/limbo_generation_range/current"
            }
        }
    ]
}